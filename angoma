import java.util.Arrays;
public class Main
{
    public static void main (String[] args) 
    {
        String a="kusu";
        String b="janu";
        char[]a1=a.toCharArray();
        char[]b1=b.toCharArray();
        Arrays.sort(a1);
        Arrays.sort(b1);
        boolean res=Arrays.equals(a1,b1);
        if(res)
        {
            System.out.print("yes");
        }
        else
        {
            System.out.print("no");
        }
    }
}
